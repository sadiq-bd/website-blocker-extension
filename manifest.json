{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Block specific websites with manual input.",
  "permissions": [
    "storage",
    "declarativeNetRequest"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
